                </div>
            </div>

            <input type="password" id="teacherPass" class="glass-input w-full rounded-lg p-3 mb-3 text-center" placeholder="Password Pengawas">
            <button onclick="ExamSystem.unlock()" class="w-full bg-slate-800 hover:bg-slate-700 text-white font-medium py-3 rounded-lg transition border border-white/10">Buka Akses (Admin)</button>
        </div>
    </div>

    <!-- MODAL: CONFIRM FINISH -->
    <div id="modal-confirm" class="modal fixed inset-0 z-50 flex items-center justify-center bg-black/60 backdrop-blur-sm p-4">
        <div class="glass-card w-full max-w-sm p-6 rounded-2xl text-center">
            <h3 class="text-lg font-bold text-white mb-2">Akhiri Sesi Ujian?</h3>
            <p class="text-slate-400 text-sm mb-6">Pastikan Anda telah menekan tombol <strong class="text-indigo-400">KIRIM</strong> di dalam formulir sebelum keluar.</p>
            <div class="flex gap-3">
                <button onclick="document.getElementById('modal-confirm').classList.remove('active')" class="flex-1 py-2.5 rounded-lg bg-white/5 hover:bg-white/10 text-slate-300 text-sm font-medium transition">Batal</button>
                <button onclick="ExamSystem.finishConfirmed()" class="flex-1 py-2.5 rounded-lg bg-red-600 hover:bg-red-500 text-white font-medium text-sm transition shadow-lg shadow-red-900/30">Ya, Selesai</button>
            </div>
        </div>
    </div>

    <!-- SIDEBAR (RESPONSIVE: TOP ON MOBILE, LEFT ON DESKTOP) -->
    <aside class="glass-card flex flex-col z-20 md:w-[320px] md:h-full border-b md:border-b-0 md:border-r border-white/5 relative bg-slate-900/40">
        
        <!-- Sidebar Header & Identity -->
        <div class="p-5 border-b border-white/5 flex items-center justify-between md:justify-start gap-4">
            <div class="flex items-center gap-3">
                <img src="https://i.postimg.cc/fy6jxRL4/Gemini-Generated-Image-j4hlrjj4hlrjj4hl.png" 
                     class="w-9 h-9 object-contain rounded bg-white/5 p-0.5 border border-white/10 shadow-sm" alt="Logo">
                <div>
                    <div class="font-bold text-sm tracking-tight text-white leading-none">Proctor<span class="text-indigo-400">View</span></div>
                    <div class="text-[10px] text-slate-500 font-medium tracking-wide mt-0.5">SECURE BROWSER</div>
                </div>
            </div>
            <!-- Mobile Timer (Visible only on small screens) -->
            <div class="md:hidden font-mono font-bold text-indigo-400 text-lg" id="timer-mobile">00:00</div>
        </div>

        <!-- Scrollable Content Area -->
        <div class="flex-1 overflow-y-auto scrollbar-hide p-5 space-y-5">
            
            <!-- Webcam Monitor -->
            <div class="relative rounded-xl overflow-hidden bg-black shadow-lg ring-1 ring-white/10 group">
                <div class="absolute top-3 left-3 z-10 flex items-center gap-1.5 bg-black/60 backdrop-blur-md px-2 py-0.5 rounded-full border border-white/10">
                    <div class="w-2 h-2 bg-red-500 rounded-full recording-dot"></div> 
                    <span class="text-[9px] font-bold text-white tracking-wider">LIVE</span>
                </div>
                <video id="webcam" autoplay muted playsinline class="w-full aspect-video object-cover transform -scale-x-100 opacity-80 group-hover:opacity-100 transition duration-500"></video>
            </div>

            <!-- Session Stats Card -->
            <div class="bg-white/[0.02] border border-white/5 rounded-xl p-4 space-y-3">
                <div class="flex items-center justify-between">
                    <div class="flex items-center gap-2">
                        <div class="w-6 h-6 rounded bg-slate-800 flex items-center justify-center text-[10px]">üë§</div>
                        <div class="text-xs font-medium text-slate-300 truncate max-w-[100px]" id="dispName">-</div>
                    </div>
                    <div class="hidden md:block font-mono font-bold text-xl text-indigo-400" id="timer">00:00</div>
                </div>
                
                <div class="h-px bg-white/5 w-full"></div>

                <!-- Integrity Meter -->
                <div>
                    <div class="flex justify-between items-center mb-1.5">
                        <span class="text-[10px] text-slate-500 font-bold uppercase tracking-wider">Integritas</span>
                        <span id="violText" class="text-[10px] font-bold text-emerald-400">100% Aman</span>
                    </div>
                    <div class="h-1.5 bg-slate-800 rounded-full overflow-hidden">
                        <div id="violBar" class="h-full w-full bg-emerald-500 shadow-[0_0_8px_rgba(16,185,129,0.5)] transition-all duration-500"></div>
                    </div>
                </div>
            </div>

            <!-- System Log (Compact) -->
            <div class="bg-black/20 rounded-xl p-3 border border-white/5 h-32 flex flex-col">
                <div class="text-[9px] font-bold text-slate-600 uppercase mb-2">Activity Log</div>
                <div id="logFeed" class="flex-1 overflow-y-auto space-y-1.5 font-mono text-[9px] text-slate-500 scrollbar-hide">
                    <div>[System] Ready to start...</div>
                </div>
            </div>
        </div>

        <!-- Footer Actions -->
        <div class="p-5 mt-auto border-t border-white/5 bg-slate-900/50">
            <button onclick="ExamSystem.finishCheck()" class="group w-full flex items-center justify-center gap-2 py-3 rounded-xl bg-white/5 hover:bg-red-500/10 hover:border-red-500/30 border border-transparent transition-all text-slate-300 hover:text-red-400 text-sm font-medium">
                <svg class="w-4 h-4 text-slate-500 group-hover:text-red-400 transition" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 16l4-4m0 0l-4-4m4 4H7m6 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h4a3 3 0 013 3v1"></path></svg>
                <span>Selesai Ujian</span>
            </button>
            <a id="dlLink" class="hidden block text-center mt-3 text-[10px] text-indigo-400 hover:text-indigo-300 cursor-pointer transition">Simpan Bukti Video</a>
        </div>
    </aside>

    <!-- MAIN EXAM AREA -->
    <main class="flex-1 relative bg-slate-100 h-full z-10">
        <!-- Loader Overlay -->
        <div id="iframe-loader" class="absolute inset-0 flex flex-col items-center justify-center bg-slate-50 z-0">
            <div class="w-8 h-8 border-2 border-slate-200 border-t-indigo-600 rounded-full animate-spin mb-2"></div>
            <div class="text-slate-400 text-xs font-medium tracking-wide">Memuat Soal...</div>
        </div>
        
        <iframe id="examFrame" class="relative z-10 w-full h-full border-none shadow-inner" allow="camera; microphone; fullscreen"></iframe>
    </main>

<script>
const CONFIG = {
    formUrl: "https://forms.gle/uEN6GRg8ftN8Aquw8", 
    token: "12345",         
    adminPass: "admin",     
    maxViolations: 3        
};

/* Modern Toast Notification */
const Toast = {
    show: (msg, type = 'info') => {
        const container = document.getElementById('toast-container');
        const el = document.createElement('div');
        
        // Styling based on type
        const colors = type === 'error' ? 'bg-red-500/10 border-red-500/20 text-red-200' : 'bg-slate-800/90 border-white/10 text-white';
        const icon = type === 'error' ? '‚ö†Ô∏è' : '‚ÑπÔ∏è';

        el.className = `toast glass-card flex items-center gap-3 px-4 py-3 rounded-lg border shadow-lg backdrop-blur-md text-sm font-medium ${colors}`;
        el.innerHTML = `<span>${icon}</span><span>${msg}</span>`;
        
        container.appendChild(el);
        requestAnimationFrame(() => el.classList.add('show'));

        setTimeout(() => {
            el.classList.remove('show');
            setTimeout(() => el.remove(), 400);
        }, 3500);
    }
};

const ExamSystem = (() => {
    let state = {
        name: '',
        violations: 0,
        locked: false,
        startTime: null,
        mediaRecorder: null,
        chunks: [],
        timerInterval: null
    };

    const dom = {
        setupModal: document.getElementById('modal-setup'),
        lockModal: document.getElementById('modal-lock'),
        confirmModal: document.getElementById('modal-confirm'),
        frame: document.getElementById('examFrame'),
        webcam: document.getElementById('webcam'),
        logFeed: document.getElementById('logFeed'),
        dispName: document.getElementById('dispName'),
        timer: document.getElementById('timer'),
        timerMobile: document.getElementById('timer-mobile'),
        violBar: document.getElementById('violBar'),
        violText: document.getElementById('violText'),
        lockViolCount: document.getElementById('lock-viol-count'),
        dlLink: document.getElementById('dlLink')
    };

    const log = (msg, isAlert = false) => {
        const time = new Date().toLocaleTimeString('id-ID', {hour:'2-digit', minute:'2-digit'});
        const el = document.createElement('div');
        el.className = `flex gap-2 ${isAlert ? 'text-red-400' : ''}`;
        el.innerHTML = `<span class="opacity-50 select-none">${time}</span> <span>${msg}</span>`;
        dom.logFeed.prepend(el);
    };

    const formatFormUrl = (url) => {
        try {
            const u = new URL(url);
            if(u.hostname.includes('google.com') && u.pathname.includes('forms')) {
                u.searchParams.set('embedded', 'true');
            }
            return u.href;
        } catch { return url; }
    };

    const saveState = () => {
        localStorage.setItem('examState', JSON.stringify({
            name: state.name,
            violations: state.violations,
            startTime: state.startTime,
            isLocked: state.locked
        }));
    };

    return {
        init: async () => {
            const nameIn = document.getElementById('inName').value.trim();
            const tokenIn = document.getElementById('inToken').value.trim();
            const urlParams = new URLSearchParams(window.location.search);

            if (!nameIn) return Toast.show('Nama lengkap wajib diisi', 'error');
            if (tokenIn !== CONFIG.token && !urlParams.get('skip')) return Toast.show('Token akses tidak valid', 'error');

            try {
                if (!document.fullscreenElement) {
                    await document.documentElement.requestFullscreen();
                }
            } catch(e) { console.log('Fullscreen blocked by user/browser'); }

            state.name = nameIn;
            state.startTime = Date.now();
            
            // Session Recovery
            const saved = localStorage.getItem('examState');
            if(saved) {
                const p = JSON.parse(saved);
                if(p.name === state.name && (Date.now() - p.startTime) < 86400000) {
                    state.violations = p.violations;
                    state.startTime = p.startTime;
                    if(p.isLocked) { 
                        state.locked = true; 
                        dom.lockModal.classList.add('active'); 
                    }
                    Toast.show('Sesi sebelumnya dipulihkan');
                }
            }

            dom.dispName.innerText = state.name;
            dom.setupModal.classList.remove('active');
            
            const customForm = urlParams.get('form');
            dom.frame.src = formatFormUrl(customForm || CONFIG.formUrl);

            await ExamSystem.startCamera();
            ExamSystem.startMonitoring();
            ExamSystem.updateUI();
            
            state.timerInterval = setInterval(() => {
                const diff = Math.floor((Date.now() - state.startTime) / 1000);
                const m = Math.floor(diff / 60).toString().padStart(2, '0');
                const s = (diff % 60).toString().padStart(2, '0');
                const timeStr = `${m}:${s}`;
                dom.timer.innerText = timeStr;
                if(dom.timerMobile) dom.timerMobile.innerText = timeStr;
                saveState(); 
            }, 1000);

            log('System initialized. Exam started.');
        },

        startCamera: async () => {
            try {
                const stream = await navigator.mediaDevices.getUserMedia({ video: true, audio: false });
                dom.webcam.srcObject = stream;
                state.mediaRecorder = new MediaRecorder(stream, { mimeType: 'video/webm' });
                state.mediaRecorder.ondataavailable = e => {
                    if(e.data.size > 0) state.chunks.push(e.data);
                };
                state.mediaRecorder.start(3000);
            } catch (err) {
                Toast.show('Akses kamera ditolak!', 'error');
                log('Camera access denied', true);
            }
        },

        startMonitoring: () => {
            document.addEventListener('visibilitychange', () => {
                if (document.hidden) ExamSystem.addViolation('Tab Switching Detected');
            });

            window.addEventListener('blur', () => {
                setTimeout(() => {
                    if (document.activeElement !== dom.frame) {
                        ExamSystem.addViolation('Screen Focus Lost');
                    }
                }, 500);
            });

            window.addEventListener('keydown', (e) => {
                const forbidden = ['F12', 'PrintScreen', 'Escape', 'Alt', 'Tab', 'Meta'];
                const isCopyPaste = (e.ctrlKey || e.metaKey) && ['c','v','x'].includes(e.key.toLowerCase());
                
                if (forbidden.includes(e.key) || isCopyPaste) {
                    e.preventDefault();
                    ExamSystem.addViolation(`Prohibited Key: ${e.key}`);
                }
            });
            
            // Fullscreen check loop
            setInterval(() => {
                if(!document.fullscreenElement && !state.locked) {
                    // Silent logging or explicit warning can be added here
                }
            }, 3000);
        },

        addViolation: (reason) => {
            if (state.locked) return;
            
            state.violations++;
            ExamSystem.updateUI();
            
            Toast.show(`Pelanggaran: ${reason}`, 'error');
            log(`VIOLATION: ${reason}`, true);
            saveState();

            if (state.violations >= CONFIG.maxViolations) {
                ExamSystem.lock();
            }
        },

        updateUI: () => {
            const max = CONFIG.maxViolations;
            const current = state.violations;
            const percentage = 100 - Math.min((current / max) * 100, 100);
            
            // Update Bar Width
            dom.violBar.style.width = `${percentage}%`;
            
            // Update Colors & Text based on risk
            if(current === 0) {
                dom.violText.innerText = "100% Aman";
                dom.violText.className = "text-[10px] font-bold text-emerald-400";
                dom.violBar.className = "h-full w-full bg-emerald-500 shadow-[0_0_10px_rgba(16,185,129,0.5)] transition-all duration-500";
            } else if (current < max) {
                dom.violText.innerText = `Resiko Sedang (${current}/${max})`;
                dom.violText.className = "text-[10px] font-bold text-orange-400";
                dom.violBar.className = "h-full w-full bg-orange-500 shadow-[0_0_10px_rgba(249,115,22,0.5)] transition-all duration-500";
            } else {
                dom.violText.innerText = "AKSES DIBEKUKAN";
                dom.violText.className = "text-[10px] font-bold text-red-500";
                dom.violBar.className = "h-full w-full bg-red-600 shadow-[0_0_10px_rgba(220,38,38,0.5)] transition-all duration-500";
            }
        },

        lock: () => {
            state.locked = true;
            dom.lockViolCount.innerText = state.violations;
            dom.lockModal.classList.add('active');
            if (document.fullscreenElement) document.exitFullscreen().catch(()=>{});
            log('SESSION LOCKED: Max violations reached', true);
        },

        unlock: () => {
            const pass = document.getElementById('teacherPass').value;
            if (pass === CONFIG.adminPass) {
                state.locked = false;
                state.violations = 0; 
                dom.lockModal.classList.remove('active');
                document.getElementById('teacherPass').value = '';
                ExamSystem.updateUI();
                log('Session unlocked by Admin');
                document.documentElement.requestFullscreen().catch(()=>{});
            } else {
                Toast.show('Password Admin Salah', 'error');
            }
        },

        finishCheck: () => {
            dom.confirmModal.classList.add('active');
        },

        finishConfirmed: () => {
            dom.confirmModal.classList.remove('active');
            if (state.mediaRecorder && state.mediaRecorder.state !== 'inactive') {
                state.mediaRecorder.stop();
            }
            clearInterval(state.timerInterval);
            localStorage.removeItem('examState');
            dom.frame.remove(); 
            
            log('Exam session finished');
            Toast.show('Memproses rekaman bukti...', 'info');

            setTimeout(() => {
                const blob = new Blob(state.chunks, { type: 'video/webm' });
                const url = URL.createObjectURL(blob);
                const filename = `Exam_${state.name.replace(/\s+/g,'_')}.webm`;
                
                dom.dlLink.href = url;
                dom.dlLink.download = filename;
                dom.dlLink.classList.remove('hidden');
                dom.dlLink.innerHTML = `<span class="inline-flex items-center gap-2 bg-indigo-600 hover:bg-indigo-500 text-white px-4 py-2 rounded-lg font-bold shadow-lg transition transform hover:-translate-y-0.5 text-xs">
                    <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16v1a3 3 0 003 3h10a3 3 0 003-3v-1m-4-4l-4 4m0 0l-4-4m4 4V4"></path></svg>
                    Unduh Bukti Video
                </span>`;
            }, 1000);
        }
    };
})();
</script>
</body>
</html>
